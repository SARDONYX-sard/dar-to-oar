{
  "name": "dar-to-oar",
  "description": "Converter",
  "version": "0.10.2",
  "license": "MIT",
  "homepage": "https://github.com/SARDONYX-sard/dar-to-oar",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf ./gui/frontend/out && tauri build",
    "build:front": "next build ./gui/frontend",
    "build:icons": "cargo tauri icon ./gui/backend/icons/icon.svg",
    "dev": "tauri dev",
    "dev:front": "next ./gui/frontend",
    "fmt": "biome format --write ./ && prettier --cache --write \"**/*.{yaml,yml}\" --ignore-path ./.gitignore && cargo fmt --all",
    "lint": "next lint ./gui/frontend && biome lint ./ && cargo clippy --workspace",
    "lint:fix": "npm run fmt && biome check --write ./ && next lint ./gui/frontend --fix && cargo clippy --workspace --fix --allow-staged --allow-dirty",
    "tauri": "tauri",
    "test": "vitest run",
    "test:all": "npm test && npm run test:back",
    "test:back": " cargo test --workspace",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "11.14.1",
    "@fontsource/inter": "5.2.6",
    "@monaco-editor/react": "4.7.0",
    "@mui/icons-material": "7.3.1",
    "@mui/lab": "7.0.0-beta.16",
    "@mui/material": "7.3.1",
    "@tauri-apps/api": "2.8.0",
    "@tauri-apps/plugin-dialog": "2.3.3",
    "@tauri-apps/plugin-fs": "2.4.2",
    "@tauri-apps/plugin-shell": "2.3.1",
    "i18next": "25.4.2",
    "next": "15.5.7",
    "notistack": "3.0.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-i18next": "15.7.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@tauri-apps/cli": "2.8.3",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react-swc": "4.0.1",
    "better-typescript-lib": "^2.11.0",
    "eslint": "^8",
    "eslint-config-next": "15.5.2",
    "jsdom": "26.1.0",
    "prettier": "^3",
    "rimraf": "^6",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "overrides": {
    "monaco-editor": "^0.49.0"
  },
  "packageManager": "npm@10.8.3"
}
