{
  "name": "dar-to-oar",
  "version": "0.2.2",
  "license": "MIT",
  "homepage": "https://github.com/SARDONYX-sard/dar-to-oar",
  "private": true,
  "scripts": {
    "dev": "tauri dev",
    "dev:front": "next frontend",
    "build": "rimraf ./out && cargo tauri build",
    "build:front": "next build frontend",
    "build:icons": "cargo tauri icon ./src-tauri/icons/icon.png",
    "test": "cargo test --workspace",
    "fmt": "cargo fmt --all",
    "lint:front": "next lint frontend",
    "lint:back": "cargo clippy --workspace",
    "lint": "npm run lint:front && npm run lint:back",
    "lint-fix:back": "cargo clippy --workspace --fix --allow-staged --allow-dirty",
    "lint-fix:front": "next lint frontend --fix",
    "lint:fix": "npm run fmt && npm run lint-fix:front && npm run lint-fix:back",
    "tauri": "tauri"
  },
  "dependencies": {
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "5.0.0-alpha.151",
    "@mui/material": "^5.14.16",
    "@tauri-apps/api": "^1.5.1",
    "ace-builds": "^1.31.1",
    "i18next": "^23.6.0",
    "next": "14.0.1",
    "react": "18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.3.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.1",
    "rimraf": "^5.0.5",
    "typescript": "5.2.2"
  }
}
